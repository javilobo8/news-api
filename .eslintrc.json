{
  "env": {
    "commonjs": true,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "no-use-before-define": "off",
    "global-require": "off",
    "no-console": "off",
    "arrow-parens": "off",
    "no-underscore-dangle": "off"
  }
}